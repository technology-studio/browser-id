{
  "name": "@txo/browser-id",
  "version": "0.0.0",
  "description": "Technology Studio - Browser id",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/browser-id.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn browserify src/index.ts -p [ tsify --noImplicitAny ] -o lib/index.js",
    "build:watch": "concurrently \"yarn build:watch:lib\"",
    "build:watch:lib": "yarn watchify src/index.ts -p [ tsify --noImplicitAny ] -o lib/index.js",
    "prepare": "yarn build"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/uuid": "^8.3.1"
  }
}
